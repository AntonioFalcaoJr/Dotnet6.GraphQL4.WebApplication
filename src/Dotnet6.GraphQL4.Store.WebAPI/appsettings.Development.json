{
  "SqlServerRetryingOptions": {
    "MaxRetryCount": 5,
    "MaxSecondsRetryDelay": 5,
    "ErrorNumbersToAdd": []
  },
  "TransactionsOptions": {
    "IsolationLevel": "ReadCommitted"
  },
  "HealthChecksPatterns": {
    "Health": "/health",
    "Liveness": "/health/live",
    "Readiness": "/health/ready",
    "UI": "/healthz"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  }
}
